
<div class="contact">


    <div class="row">
        <div class="col-md-12">
            <h3> @Resource.ContctUs : </h3>
        </div>

        <div class="col-md-6">
            <div class="form-group">
                <label>@Resource.Email</label>
                <label class="form-control">@ProfileMapper.Get().Name</label>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label>@Resource.Phone</label>
                <label class="form-control">@ProfileMapper.Get().Phones</label>
            </div>
        </div>

        <div class="col-md-12">
            <div class="form-group">
                <label>@Resource.Address</label>
                <label class="form-control">@ProfileMapper.Get().Address</label>
            </div>
        </div>
    </div>
</div>